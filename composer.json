{
  "name": "tormjens/posteio",
  "description": "A PHP SDK for the Poste.io API with support for Laravel 5",
  "keywords": [
    "poste.io",
    "mail",
    "laravel"
  ],
  "license": "MIT",
  "type": "package",
  "require": {
    "php": ">=7.1.0",
    "psr/http-message": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/httplug": "^1.0",
    "php-http/message-factory": "^1.0",
    "php-http/discovery": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "php-http/mock-client": "^1.0",
    "php-http/message": "^1.0",
    "guzzlehttp/psr7": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "TorMorten\\Posteio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PosteioTests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TorMorten\\Posteio\\Providers\\PosteioServiceProvider"
      ],
      "aliases": {
        "Posteio": "TorMorten\\Posteio\\Posteio"
      }
    }
  }
}
